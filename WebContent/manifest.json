{
  "name": "Virtual Keyboard 재문",
  "manifest_version": 2,
  "version": "1.0",
  "author": "KimJaeMoon",
  "default_locale": "ko",
  "description": "Virtual Keyboard",
  "icons": {"16": "img/16.png",
		  	"128":"img/128.png"
		  },

  "devtools_page": "setting.html",
  
  "background" : {
    "scripts" : ["js/background.js"],
    "persistent" : false
  },
  "browser_action": {
	"default_popup" : "setting.html",
  	"default_icon" : "img/128.png"
  },

  "content_scripts": [
  	{"matches" : ["<all_urls>"],
  	"css":["css/main.css"],
  	"js":["js/jquery-3.5.1.slim.js","js/content.js"]
  	}
  ],
  
  "permissions": [
  	"activeTab"
  ],
  "version_name": "1.0.0"
}